{
    "compilerOptions": {
        "declaration": true,
        "target": "ESNEXT",
        "jsx": "react",
        "module": "CommonJS",
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "preserveConstEnums": true,
        "stripInternal": true
    },
    "exclude": [
        "node_modules"
    ]
  }